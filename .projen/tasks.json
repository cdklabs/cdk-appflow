      "description": "deploy integration test 'ondemand-googleanalytics4-to-s3' and capture snapshot",
      "steps": [
        {
          "exec": "rm -fr test/integ/.tmp/ondemand-googleanalytics4-to-s3.integ/deploy.cdk.out"
        },
        {
          "exec": "cdk deploy --app \"ts-node -P tsconfig.dev.json test/integ/ondemand-googleanalytics4-to-s3.integ.ts\" --no-notices --no-version-reporting --no-asset-metadata --no-path-metadata '**' --require-approval=never -o test/integ/.tmp/ondemand-googleanalytics4-to-s3.integ/deploy.cdk.out"
        },
